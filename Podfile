# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'GithubNotifier' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GithubNotifier
  pod 'Alamofire', '~> 4.4' # HTTP Requesting Library
  pod 'SwiftyJSON' # handle JSON from the API
  pod "STRegex", "~> 1.0" # easy regex library
end
